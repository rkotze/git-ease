{
  "name": "git-ease",
  "displayName": "Git Ease",
  "description": "Making it easy to make good commits",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:git-ease.helloWorld",
    "onView:gitEasePanel"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-ease.helloWorld",
        "title": "Hello World"
      }
    ],
    "views": {
      "scm": [
        {
          "type": "webview",
          "id": "gitEasePanel",
          "name": "Git ease"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@primer/octicons": "^11.1.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "svelte": "^3.29.7",
    "svelte-loader": "^2.13.6",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
